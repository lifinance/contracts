pragma solidity 0.8.17;

import { LibAsset } from "lifi/Libraries/LibAsset.sol";
import { TestBaseFacet } from "../utils/TestBaseFacet.sol";
import { LibAllowList } from "lifi/Libraries/LibAllowList.sol";
import { CatalystFacet, ITheCompact, TheCompactOrderType, OutputDescription, CatalystCompactOrder } from "lifi/Facets/CatalystFacet.sol";

contract AlwaysOKAllocator {
    function attest(
        address,
        address,
        address,
        uint256,
        uint256
    ) external pure returns (bytes4) {
        return this.attest.selector;
    }

    function authorizeClaim(
        bytes32,
        address,
        address,
        uint256,
        uint256,
        uint256[2][] calldata,
        bytes calldata
    ) external pure returns (bytes4) {
        return this.authorizeClaim.selector;
    }

    function isClaimAuthorized(
        bytes32,
        address,
        address,
        uint256,
        uint256,
        uint256[2][] calldata,
        bytes calldata
    ) external pure returns (bool) {
        return true;
    }
}

contract AlwaysYesOracle {
    function isProven(
        uint256,
        /* remoteChainId */ bytes32,
        /* remoteOracle */ bytes32,
        /* application */ bytes32 /* dataHash */
    ) external pure returns (bool) {
        return true;
    }

    function efficientRequireProven(
        bytes calldata /* proofSeries */
    ) external pure {}
}

interface ImmutableCreate2Factory {
    function safeCreate2(
        bytes32 salt,
        bytes calldata initializationCode
    ) external payable returns (address deploymentAddress);
}

interface ICatalystCompactSettler {
    function finaliseSelf(
        CatalystCompactOrder calldata order,
        bytes calldata signatures,
        uint32[] calldata timestamps,
        bytes32 solver
    ) external;

    function orderIdentifier(
        CatalystCompactOrder calldata order
    ) external view returns (bytes32);
}

// Stub CatalystFacet Contract
contract TestCatalystFacet is CatalystFacet {
    constructor(
        address compact,
        address compactSettler
    ) CatalystFacet(compact, compactSettler) {}

    function addDex(address _dex) external {
        LibAllowList.addAllowedContract(_dex);
    }

    function setFunctionApprovalBySignature(bytes4 _signature) external {
        LibAllowList.addAllowedSelector(_signature);
    }
}

contract CatalystFacetTest is TestBaseFacet {
    CatalystFacet.CatalystData internal validCatalystData;
    TestCatalystFacet internal catalystFacet;
    TestCatalystFacet internal baseCatalystFacet;

    address internal compact;
    address internal catalystCompactSettler;

    address internal alwaysOkAllocator;
    address internal alwaysYesOracle;

    bytes12 internal lockId;

    function deployDependencies() internal {
        address immutableCreate2Factory = address(
            0x0000000000FFe8B47B3e2130213B802212439497
        );
        bytes
            memory immutableCreate2FactoryRuntimeCode = hex"";
        vm.etch(immutableCreate2Factory, immutableCreate2FactoryRuntimeCode);

        bytes32 salt = bytes32(
            0x00000000000000000000000000000000000000008a0f466a78cd1102ce3d82f7
        );
        bytes
            memory theCompactCreationCode = hex"";

        compact = ImmutableCreate2Factory(immutableCreate2Factory).safeCreate2(
            salt,
            theCompactCreationCode
        );

        bytes
            memory catalystCompactSettlerCreationCode = hex"";

        catalystCompactSettler = ImmutableCreate2Factory(
            immutableCreate2Factory
        ).safeCreate2(
                salt,
                abi.encodePacked(
                    catalystCompactSettlerCreationCode,
                    abi.encode(compact)
                )
            );
    }

    function setUp() public {
        customBlockNumberForForking = 17130542;
        initTestBase();

        // deploy oracle & allocator
        alwaysYesOracle = address(new AlwaysYesOracle());
        alwaysOkAllocator = address(new AlwaysOKAllocator());

        deployDependencies();

        // Register allocator.
        uint96 allocatorid = ITheCompact(compact).__registerAllocator(
            alwaysOkAllocator,
            hex""
        );
        // Use 0 reset period and scope. This works the time is not moved forward.
        lockId = bytes12(allocatorid);

        baseCatalystFacet = new TestCatalystFacet(
            compact,
            catalystCompactSettler
        );

        bytes4[] memory functionSelectors = new bytes4[](4);
        functionSelectors[0] = baseCatalystFacet
            .startBridgeTokensViaCatalyst
            .selector;
        functionSelectors[1] = baseCatalystFacet
            .swapAndStartBridgeTokensViaCatalyst
            .selector;
        functionSelectors[2] = baseCatalystFacet.addDex.selector;
        functionSelectors[3] = baseCatalystFacet
            .setFunctionApprovalBySignature
            .selector;

        addFacet(diamond, address(baseCatalystFacet), functionSelectors);
        catalystFacet = TestCatalystFacet(address(diamond));
        catalystFacet.addDex(ADDRESS_UNISWAP);
        catalystFacet.setFunctionApprovalBySignature(
            uniswap.swapExactTokensForTokens.selector
        );
        catalystFacet.setFunctionApprovalBySignature(
            uniswap.swapTokensForExactETH.selector
        );
        catalystFacet.setFunctionApprovalBySignature(
            uniswap.swapETHForExactTokens.selector
        );

        setFacetAddressInTestBase(address(catalystFacet), "CatalystFacet");

        // adjust bridgeData
        bridgeData.bridge = "catalyst";
        bridgeData.destinationChainId = 137;

        // produce valid CatalystData
        validCatalystData = CatalystFacet.CatalystData({
            receiverAddress: bytes32(uint256(uint160(bridgeData.receiver))),
            assetId: uint256(0), // needs to be overwritte on test
            expectedClaimHash: bytes32(0),
            user: address(uint160(123123321321)),
            nonce: uint256(100),
            expiry: type(uint32).max,
            // Catalyst Witness //
            fillDeadline: type(uint32).max,
            localOracle: alwaysYesOracle, // Not used
            // Catalyst Output //
            remoteOracle: bytes32(0), // not used
            remoteFiller: bytes32(0), // not used
            outputToken: bytes32(uint256(888999888)),
            outputAmount: 999888999,
            remoteCall: hex"",
            fulfillmentContext: hex""
        });
    }

    // All facet test files inherit from `utils/TestBaseFacet.sol` and require the following method overrides:
    // - function initiateBridgeTxWithFacet(bool isNative)
    // - function initiateSwapAndBridgeTxWithFacet(bool isNative)
    //
    // These methods are used to run the following tests which must pass:
    // - testBase_CanBridgeNativeTokens()
    // - testBase_CanBridgeTokens()
    // - testBase_CanBridgeTokens_fuzzed(uint256)
    // - testBase_CanSwapAndBridgeNativeTokens()
    // - testBase_CanSwapAndBridgeTokens()
    // - testBase_Revert_BridgeAndSwapWithInvalidReceiverAddress()
    // - testBase_Revert_BridgeToSameChainId()
    // - testBase_Revert_BridgeWithInvalidAmount()
    // - testBase_Revert_BridgeWithInvalidDestinationCallFlag()
    // - testBase_Revert_BridgeWithInvalidReceiverAddress()
    // - testBase_Revert_CallBridgeOnlyFunctionWithSourceSwapFlag()
    // - testBase_Revert_CallerHasInsufficientFunds()
    // - testBase_Revert_SwapAndBridgeToSameChainId()
    // - testBase_Revert_SwapAndBridgeWithInvalidAmount()
    // - testBase_Revert_SwapAndBridgeWithInvalidSwapData()
    //
    // In some cases it doesn't make sense to have all tests. For example the bridge may not support native tokens.
    // In that case you can override the test method and leave it empty. For example:
    //
    // function testBase_CanBridgeNativeTokens() public override {
    //     // facet does not support bridging of native assets
    // }
    //
    // function testBase_CanSwapAndBridgeNativeTokens() public override {
    //     // facet does not support bridging of native assets
    // }

    event Transfer(
        address by,
        address indexed from,
        address indexed to,
        uint256 indexed id,
        uint256 amount
    );

    event CompactRegistered(
        address indexed sponsor,
        bytes32 claimHash,
        bytes32 typehash
    );

    event Finalised(
        bytes32 indexed orderId,
        bytes32 solver,
        bytes32 destination
    );

    function test_catalyst_deposit_on_compact() external {
        bool isNative = false;
        vm.startPrank(USER_SENDER);
        usdc.approve(address(baseCatalystFacet), bridgeData.minAmount);

        bridgeData.sendingAssetId = isNative ? address(0) : address(usdc);

        validCatalystData.assetId = uint256(
            bytes32(abi.encodePacked(lockId, bridgeData.sendingAssetId))
        );

        // Check that the execution happens as we would expect it to.

        address by = address(baseCatalystFacet);
        address from = address(0); // indexed
        address to = validCatalystData.user; // indexed
        uint256 id = validCatalystData.assetId; // indexed
        uint256 amount = bridgeData.minAmount;

        vm.expectEmit();
        emit Transfer(by, from, to, id, amount);

        OutputDescription[] memory outputs = new OutputDescription[](1);
        outputs[0] = OutputDescription({
            remoteOracle: validCatalystData.remoteOracle,
            remoteFiller: validCatalystData.remoteFiller,
            chainId: bridgeData.destinationChainId,
            token: validCatalystData.outputToken,
            amount: validCatalystData.outputAmount,
            recipient: validCatalystData.receiverAddress,
            remoteCall: validCatalystData.remoteCall,
            fulfillmentContext: validCatalystData.fulfillmentContext
        });
        uint256[2][] memory idsAndAmounts = new uint256[2][](1);
        idsAndAmounts[0] = [validCatalystData.assetId, bridgeData.minAmount];

        address sponsor = validCatalystData.user; // indexed
        bytes32 typehash = TheCompactOrderType
            .CATALYST_BATCH_COMPACT_TYPE_HASH;
        bytes32 claimHash = keccak256(
            abi.encode(
                typehash,
                catalystCompactSettler,
                validCatalystData.user,
                validCatalystData.nonce,
                validCatalystData.expiry,
                keccak256(abi.encodePacked(idsAndAmounts)),
                TheCompactOrderType.witnessHash(
                    validCatalystData.fillDeadline,
                    validCatalystData.localOracle,
                    outputs
                )
            )
        );

        vm.expectEmit();
        emit CompactRegistered(sponsor, claimHash, typehash);

        baseCatalystFacet.startBridgeTokensViaCatalyst{
            value: LibAsset.isNativeAsset(bridgeData.sendingAssetId)
                ? bridgeData.minAmount
                : 0
        }(bridgeData, validCatalystData);
        vm.stopPrank();

        // Check that we can redeem the intent (i.e. that we registered the intent we expected.)

        address solver = address(7788778877);
        vm.startPrank(solver);

        CatalystCompactOrder memory order = CatalystCompactOrder({
            user: validCatalystData.user,
            nonce: validCatalystData.nonce,
            originChainId: block.chainid,
            expires: validCatalystData.expiry,
            fillDeadline: validCatalystData.fillDeadline,
            localOracle: validCatalystData.localOracle,
            inputs: idsAndAmounts,
            outputs: outputs
        });
        bytes32 orderId = ICatalystCompactSettler(catalystCompactSettler)
            .orderIdentifier(order);
        bytes memory signatures = abi.encode(hex"", hex"");
        uint32[] memory timestamps = new uint32[](1);
        timestamps[0] = type(uint32).max;
        bytes32 solverIdentifier = bytes32(uint256(uint160(solver)));

        vm.expectEmit();
        emit Finalised(orderId, solverIdentifier, solverIdentifier);

        ICatalystCompactSettler(catalystCompactSettler).finaliseSelf(
            order,
            signatures,
            timestamps,
            solverIdentifier
        );

        assertEq(usdc.balanceOf(solver), bridgeData.minAmount);
    }

    function initiateBridgeTxWithFacet(bool isNative) internal override {
        validCatalystData.assetId = uint256(
            bytes32(abi.encodePacked(lockId, bridgeData.sendingAssetId))
        );

        if (isNative) {
            catalystFacet.startBridgeTokensViaCatalyst{
                value: bridgeData.minAmount
            }(bridgeData, validCatalystData);
        } else {
            catalystFacet.startBridgeTokensViaCatalyst(
                bridgeData,
                validCatalystData
            );
        }
    }

    function initiateSwapAndBridgeTxWithFacet(
        bool isNative
    ) internal override {
        validCatalystData.assetId = uint256(
            bytes32(abi.encodePacked(lockId, bridgeData.sendingAssetId))
        );

        if (isNative) {
            catalystFacet.swapAndStartBridgeTokensViaCatalyst{
                value: swapData[0].fromAmount
            }(bridgeData, swapData, validCatalystData);
        } else {
            catalystFacet.swapAndStartBridgeTokensViaCatalyst(
                bridgeData,
                swapData,
                validCatalystData
            );
        }
    }
}
